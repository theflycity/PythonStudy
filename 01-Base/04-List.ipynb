   "source": [
    "import random\n",
    "\n",
    "counters = [0] * 6\n",
    "# 模拟掷色子记录每种点数出现的次数\n",
    "for _ in range(6000):\n",
    "    face = random.randrange(1, 7)\n",
    "    counters[face - 1] += 1\n",
    "# 输出每种点数出现的次数\n",
    "for face in range(1, 7):\n",
    "    print(f'{face}点出现了{counters[face - 1]}次')"
   ]
  }
